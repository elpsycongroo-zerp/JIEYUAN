{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "游客倒电表",
            "entry": "令常乐"
        },
        {
            "name": "刷母鼎",
            "entry": "年常乐"
        },
        {
            "name": "叠生命值",
            "entry": "令常乐s",
            "advanced": [
                "作战次数"
            ]
        },
        {
            "name": "普通任务",
            "entry": "MyTask1"
        },
        {
            "name": "选项任务",
            "entry": "MyTask2",
            "option": [
                "任务选项1"
            ]
        },
        {
            "name": "参数任务",
            "entry": "MyTask3",
            "option": [
                "任务选项1"
            ],
            "pipeline_override": {}
        },
        {
            "name": "带Custom的任务",
            "entry": "MyTask4"
        }
    ],
    "advanced": {
        "作战次数": {
            "field": [
                "作战次数"
            ],
            "type": [
                "int"
            ],
            "default": [
                "0"
            ],
            "pipeline_override": {
                "AllIn": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "target_count": "{作战次数}"
                            }
                        }
                    }
                }
            },
            "doc": "填0清空体力；填其他数字按次数刷；其他情况清空体力"
        },
        "option": {
            "任务选项1": {
                "cases": [
                    {
                        "name": "文本1",
                        "pipeline_override": {
                            "OcrTask": {
                                "text": "文本1"
                            }
                        }
                    },
                    {
                        "name": "文本2",
                        "pipeline_override": {
                            "OcrTask": {
                                "text": "文本2"
                            }
                        }
                    }
                ]
            }
        }
    }
}